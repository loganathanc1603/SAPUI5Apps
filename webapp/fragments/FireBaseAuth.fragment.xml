<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<f:Form editable="true">
		<f:title>
			<core:Title text="Firebase Login"/>
		</f:title>
		<f:layout>
			<f:ResponsiveGridLayout labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4"
				emptySpanM="4" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1" singleContainerFullSize="false"/>
		</f:layout>
		<f:formContainers>
			<f:FormContainer>
				<f:formElements>
					<f:FormElement label="Email Id">
						<f:fields>
							<Input id="iPEmailId"/>
						</f:fields>
					</f:FormElement>
					<f:FormElement label="Password">
						<f:fields>
							<Input type="Password" id="iPPwdId"/>
						</f:fields>
					</f:FormElement>
					<f:FormElement label="">
						<f:fields>
							<Button text="Sign In" type="Emphasized" press="onPrsFireAuth" app:filterId="SI"></Button>
							<Button text="Sign Up" type="Default" press="onPrsFireAuth" app:filterId="SU"></Button>
							<Button text="Sign Out" type="Reject" press="onPrsFireAuth" app:filterId="SO" visible="true"></Button>
						</f:fields>
					</f:FormElement>
				</f:formElements>
			</f:FormContainer>
		</f:formContainers>
	</f:Form>
</core:FragmentDefinition>