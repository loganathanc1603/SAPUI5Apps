			<Title text="{i18n>headerTitle}"/>
		</semantic:titleHeading>
		<semantic:content>
			<IconTabBar id="IcTbId" select="onSelectIcTb" selectedKey="iCTab_One_CdsSrv">
				<items>
					<IconTabFilter key="iCTab_One_CdsSrv" text="CDS Service" tooltip="CDS Service" icon="sap-icon://add-activity">
						<core:Fragment fragmentName="com.sap.ui5.app.ZSAPUI5ALLAPP.fragments.SFlightCDS" type="XML"></core:Fragment>
					</IconTabFilter>
					<IconTabFilter iconColor="Positive" key="iCTab_Two_FireBase" text="Firebase Exercise" tooltip="Firebase Exercise" icon="sap-icon://table-view">
						<core:Fragment fragmentName="com.sap.ui5.app.ZSAPUI5ALLAPP.fragments.FireBaseData" type="XML"></core:Fragment>
					</IconTabFilter>
					<IconTabFilter key="iCTab_One_SmartCtrl" icon="sap-icon://activate" text="{i18n>workList.iconTabBarTitleOne}"
						tooltip="{i18n>workList.iconTabBarTitleOne}" visible="false">
						<!--<smartFilterBar:SmartFilterBar id="smartFilterBar" entitySet="EmployeeHeaderSet" persistencyKey="SmartFilterPKey">-->
